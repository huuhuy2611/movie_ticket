{
  "compilerOptions": {
    "target": "es6",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "@/images":["./public/images"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "custom.d.ts", "custom_type.d.ts", "pages/_document.tsx"],
  "exclude": ["node_modules"]
}
